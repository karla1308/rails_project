              <div class="carousel-item active">
                <%= image_tag("daycity.jpg", :class=>"d-block w-100")%>
                <div class="carousel-caption d-none d-md-block">
                  <p>Picture by me</p>
                </div>
              </div>
              <div class="carousel-item" style="size:100px 100px;">
                <%= image_tag("midroad.jpg", :class=>"d-block w-100")%>
