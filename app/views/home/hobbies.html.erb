  <body style="background-color:#fcd6db;">
    
<!-- ------------------------------------------------------------------------------------------------------------------------ -->
    

  <div class="container" style="width:50%;margin:auto; height:20%;">
      <div>
        <h1 class="rounded hset" style="background-color:#8fbc8f; border: 2px solid #5C4033;"> <%= image_tag("star.gif", :class=> "imgset2")%> Hobbies! <%= image_tag("star.gif", :class=> "imgset2")%></h1>
      </div>
